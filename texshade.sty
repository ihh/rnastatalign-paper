\def\residue@count{%
    \expandafter\res@get\seq@line
    \if\first@\ampers@nd
    \else
         \advance\total@count by 1
         \advance\innerloopcount by 1
         \ifstart@ \advance\res@count by 1 \fi
         \ifnum\start@seq=0
            \advance\end@count by 1
