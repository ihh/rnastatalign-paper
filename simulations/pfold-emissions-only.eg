 (transform (from (pfoldL)) (to (pfoldF)) (prob 1))
 (transform (from (pfoldL)) (to (pfoldU)) (prob 1))

 ;; state pfoldB
 (transform (from (pfoldB)) (to (pfoldL pfoldS)))

