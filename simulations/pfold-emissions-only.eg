 (transform (from (pfoldS)) (to (pfoldB)) (prob 1))

 ;; state pfoldF
 (transform (from (pfoldF)) (to (LNUC pfoldF* RNUC)) (gaps-ok)
  (annotate (row SS_cons) (column LNUC) (label <))
  (annotate (row SS_cons) (column RNUC) (label >)))
 (transform (from (pfoldF*)) (to (pfoldF)) (prob 1))
 (transform (from (pfoldF*)) (to (pfoldB)) (prob 1))

